
 
   


<section class="sixteen wide column" style="padding-bottom:40px;">
  
   <h1 class="ui padded blue header"><i class="bookmark icon"></i>
   {{header}} {{allbookmarks.title}}
  </h1>
  </section>

<div class="ui four column grid">
    {{#each allbookmarks.bookmarks}}
  <div class="column">


<div class="ui fluid card">
      
    
      <div class="content">
        <h3>Link: <a href ="{{link}}" target="_blank">{{title}}</a></h3>
      </div>
  
      <div class="content">
        <h3>Summary </h3>{{summary}}
      </div>
  
      <div class="ui segment">
         <a href="/bookmarkList/{{../allbookmarks.id}}/deleteBookmark/{{id}}" 
             class="delbookmark ui icon button " data-tooltip="WARNING this will delete the bookmark!!"  data-position="right center">
            <i class="trash icon"></i>
          </a>
    </div>
          

      </div>
    
  </div>

{{/each}}







 